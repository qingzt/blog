{
    "version": 2,
    "builds": [
      {
        "src": "./index.py",
        "use": "@vercel/python"
      }
    ],
    "rewrites": [
       {
         "src": "/(.*)",
         "dest": "/"
       }
    ],
    "headers":[
        {
            "source": "/(.*)",
            "headers":[
                {
                    "key": "Cache-Control",
                    "value": "max-age=0ï¼Œ s-maxage=31536000"
                }
            ]
        }
    ]
}
