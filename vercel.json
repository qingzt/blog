{
    "version": 2,
    "builds": [
      {
        "src": "./index.py",
        "use": "@vercel/python"
      }
    ],
    "rewrites": [
       {
         "source": "/(.*)",
         "destination": "/index.py"
       }
    ],
    "headers":[
        {
            "source": "/(.*)",
            "headers":[
                {
                    "key": "Cache-Control",
                    "value": "max-age=0, s-maxage=31536000"
                }
            ]
        }
    ]
}
